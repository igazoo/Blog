<div class="new-user-wrapper">
  <div class="container">
    <div class="row">
      <div class="col-md-offset-4 col-md-3 users-new-container">
        <h1 class="text-center text-white"></h1>
        <%= form_for :session,url: login_path do |f| %>
          
          <div class="form-group">
            <%= f.label :email, class: 'text-white' %>
            <%= f.text_field :email, class: 'form-control' %>
          </div>
          <div class="form-group">
            <%= f.label :password, class: 'text-white' %>
            <%= f.password_field :password, class: 'form-control' %>
          </div>
          
          
          <%= f.submit "Log in", class: 'btn-block btn-white' %>
        <% end %>
        <%= link_to '新規登録はこちら', new_user_path,class: 'text-white' %>
      </div>
    </div>
  </div>
</div>